{
  "expo": {
    "name": "OnSite Worker",
    "slug": "onsite-worker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "onsite-worker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#059669"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.onsiteclub.worker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#059669"
      },
      "package": "com.onsiteclub.worker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "OnSite Worker precisa acessar sua c√¢mera para tirar fotos da obra."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "OnSite Worker precisa acessar suas fotos para enviar imagens da obra."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
