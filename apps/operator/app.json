{
  "expo": {
    "name": "OnSite Operator",
    "slug": "onsite-operator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "onsite-operator",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F2937"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onsiteclub.operator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1F2937"
      },
      "package": "com.onsiteclub.operator"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-camera", { "cameraPermission": "Permitir acesso a camera para escanear QR codes" }],
      ["expo-build-properties", {
        "android": {
          "kotlinVersion": "1.9.25",
          "compileSdkVersion": 35,
          "targetSdkVersion": 34,
          "minSdkVersion": 24
        }
      }]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
