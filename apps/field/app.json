{
  "expo": {
    "name": "OnSite Dashboard",
    "slug": "onsite-dashboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "onsite-dashboard",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F2937"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onsiteclub.dashboard"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1F2937"
      },
      "package": "com.onsiteclub.dashboard"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow OnSite Dashboard to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow OnSite Dashboard to access your photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
