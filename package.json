{
  "name": "onsite-eagle",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "dev:monitor": "turbo dev --filter=@onsite/monitor",
    "dev:field": "turbo dev --filter=@onsite/field",
    "dev:inspect": "turbo dev --filter=@onsite/inspect",
    "dev:calculator": "turbo dev --filter=@onsite/calculator",
    "dev:timekeeper": "turbo dev --filter=@onsite/timekeeper",
    "dev:auth": "turbo dev --filter=@onsite/auth-app",
    "dev:dashboard": "turbo dev --filter=@onsite/dashboard",
    "dev:analytics": "turbo dev --filter=@onsite/analytics",
    "dev:operator": "turbo dev --filter=@onsite/operator",
    "dev:architecture": "turbo dev --filter=@onsite/architecture",
    "clean": "turbo clean && rm -rf node_modules",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "format": "prettier --write \"apps/**/*.{ts,tsx,js,jsx,json,css,md}\" \"packages/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"apps/**/*.{ts,tsx,js,jsx,json,css,md}\" \"packages/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "devDependencies": {
    "metro-cache-key": "^0.81.0",
    "metro-transform-worker": "0.81.0",
    "prettier": "^3.4.0",
    "turbo": "^2.3.0"
  },
  "packageManager": "npm@10.0.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
