{
  "name": "onsite-eagle",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "dev:web": "turbo dev --filter=@onsite/web",
    "dev:dashboard": "turbo dev --filter=@onsite/dashboard",
    "dev:worker": "turbo dev --filter=@onsite/worker",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "packageManager": "npm@10.0.0"
}
